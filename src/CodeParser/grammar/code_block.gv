digraph CODE_BLOCK {
    start [label=CODE_BLOCK shape=plaintext]
    alg_unit [label=ALG_UNIT shape=box]
    operator [label=OPERATOR shape=box]
    end [label="" shape=point]
    comma [label="," shape=oval]

    start -> alg_unit
    start -> operator
    alg_unit -> comma
    comma -> alg_unit
    comma -> operator
    operator -> comma
    alg_unit -> end
    operator -> end
}