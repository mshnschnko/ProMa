digraph TRANSITION {
    start [label=TRANSITION shape=plaintext]
    next_for [label="next for" shape=oval]
    exit_for [label="exit for" shape=oval]
    goto [label="goto" shape=oval]
    end [label="" shape=point]

    start -> next_for
    start -> exit_for
    start -> goto
    next_for -> end
    exit_for -> end
    goto -> end
}
