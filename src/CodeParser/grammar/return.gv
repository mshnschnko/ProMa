digraph RETURN {
    start [label=RETURN shape=plaintext]
    return [label="return" shape=oval]
    br_open [label="(" shape=oval]
    br_close [label=")" shape=oval]
    statement [label=STATEMENT shape=box]
    end [label="" shape=point]

    start -> return
    return -> br_open
    br_open -> statement
    statement -> br_close
    br_close -> end
}